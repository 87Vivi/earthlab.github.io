---
layout:
---
<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.sitemaps.org/schemas/sitemap/0.9 http://www.sitemaps.org/schemas/sitemap/0.9/sitemap.xsd" xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
{% for post in site.posts %}
{% if post.sitemap %}
{% else %}
<url>
<loc>{{ post.url }}</loc>
<lastmod>{% if post.modified %}{{ post.modified | date_to_xmlschema }} {% else %} {{ post.date | date_to_xmlschema }} {% endif %}</lastmod>
</url>
{% endif %}
{% endfor %}

{% comment %}
{% for page in site.pages %}
<!-- begin pages-->
{% if page.sitemap %}
{% else %}
<url>
<loc>{{ page.url }}</loc>
<lastmod>{% if page.modified %}{{ page.modified | date_to_xmlschema }} {% elsif page.date %} {{ page.date | date_to_xmlschema }} {% endif %}</lastmod>
</url>

{% endif %}
{% endfor %}
{% endcomment %}
</urlset>
